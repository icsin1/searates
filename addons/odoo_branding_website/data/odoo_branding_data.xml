<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <function name="write" model="ir.model.data">
            <function name="search" model="ir.model.data">
                <value eval="[('module', '=', 'base'), ('name', 'in', ['partner_root'])]"/>
            </function>
            <value eval="{'noupdate': False}"/>
        </function>
        <record id="base.partner_root" model="res.partner">
            <field name="name">System</field>
            <field name="email">systembot@example.com</field>
            <field name="image_1920" type="base64" file="odoo_branding/static/images/systembot.png"/>
        </record>
        <function name="write" model="ir.model.data">
            <function name="search" model="ir.model.data">
                <value eval="[('module', '=', 'base'), ('name', 'in', ['partner_root'])]"/>
            </function>
            <value eval="{'noupdate': True}"/>
        </function>
        <record id="base.open_module_tree" model="ir.actions.act_window">
            <field name="domain">['!', ('name', '=like', 'theme_%'), ("to_buy", "!=", True)]</field>
        </record>
    </data>
</odoo>