<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <data noupdate="0">
        <record id="cron_shipment_quote_expiry_date" model="ir.cron">
            <field name="name">Shipment Quote Expiry Date</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall">1</field>
            <field name="model_id" ref="model_shipment_quote"/>
            <field name="state">code</field>
            <field name="code">model._cron_shipment_quote_expiry_date()</field>
            <field name="nextcall" eval="(DateTime.now().replace(hour=6, minute=15)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>
    </data>

</odoo>
