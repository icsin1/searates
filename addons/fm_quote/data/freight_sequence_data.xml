<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="fm_quote_freight_auto_sequence" model="freight.sequence">
            <field name="name">Shipment Quote</field>
            <field name="ir_model_id" ref="fm_quote.model_shipment_quote"/>
            <field name="ir_field_id" ref="fm_quote.field_shipment_quote__name"/>
            <field name="ir_sequence_id" ref="fm_quote.sequence_freight_quotation"/>
            <field name="sequence_format">QT-{{object.transport_mode_id.code}}-{{ object.shipment_type_id.code}}-{{ object.cargo_type_id.code }}</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

    </data>

</odoo>
