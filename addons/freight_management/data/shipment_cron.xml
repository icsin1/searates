<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_shipment_pre_alert_notification" model="ir.cron">
        <field name="name">Shipment Pre Alert Notification</field>
        <field name="model_id" ref="freight_management.model_freight_house_shipment"/>
        <field name="state">code</field>
        <field name="code">model._shipment_pre_auto_reminder()</field>
        <!--    <field name="user_id" ref="base.user_root" />-->
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=2)).strftime('%Y-%m-%d %H:%M:%S')" />
        <field eval="False" name="doall" />
        <field name="priority">10</field>
    </record>
</odoo>
