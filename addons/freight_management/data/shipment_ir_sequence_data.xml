<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_house_freight_shipment" model="ir.sequence">
            <field name="name">Freight House shipment Sequence</field>
            <field name="code">freight.house.shipment.sequence</field>
            <field name="prefix">-%(year)s-</field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" ref="base.main_company"/>
        </record>
        <record id="sequence_master_freight_shipment" model="ir.sequence">
            <field name="name">Freight Master shipment Sequence</field>
            <field name="code">freight.master.shipment.sequence</field>
            <field name="prefix">-%(year)s-</field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" ref="base.main_company"/>
        </record>
    </data>
</odoo>
