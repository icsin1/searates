<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="layout" inherit_id="web.layout" name="Web layout">
        <xpath expr="//title" position="replace">
            <title t-esc="title or 'SearatesERP'"/>
        </xpath>
        <xpath expr="//link[@type='image/x-icon']" position="replace">
            <link type="image/x-icon" rel="shortcut icon" t-att-href="x_icon or '/odoo_branding/static/images/favicon.ico'"/>
        </xpath>
    </template>
</odoo>
