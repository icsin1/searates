<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="rate_request_view_tree" model="ir.ui.view">
        <field name="name">rate_request.view.tree</field>
        <field name="model">rate.request</field>
        <field name="arch" type="xml">
            <tree string="Rate Requests" create="false">
                <field name="name" string="Rate Requset Number"></field>
                <field name="agent_id"></field>
                <field name="opportunity_id"></field>
            </tree>
        </field>
    </record>

    <record id="rate_request_action" model="ir.actions.act_window">
        <field name="name">Rate Requests</field>
        <field name="res_model">rate.request</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem
        id="rate_request_menu"
        name="Rate Requests"
        action="rate_request_action"
        parent="fm_sale_crm.fm_sales_root_menu"/>
</odoo>