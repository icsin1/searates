<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
        <record id="crm_prospect_lead_seq" model="ir.sequence">
            <field name="name">CRM Prospect Lead</field>
            <field name="code">crm.prospect.lead</field>
            <field name="prefix">LE/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="crm_prospect_opportunity_seq" model="ir.sequence">
            <field name="name">CRM Prospect Opportunity</field>
            <field name="code">crm.prospect.opportunity</field>
            <field name="prefix">OP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="rate_request_seq" model="ir.sequence">
            <field name="name">Rate Request</field>
            <field name="code">rate.request.code</field>
            <field name="prefix">INQAE/%(year)s/</field>
            <field name="padding">5</field>
            <field eval="False" name="company_id"/>
        </record>
    </data>

</odoo>
