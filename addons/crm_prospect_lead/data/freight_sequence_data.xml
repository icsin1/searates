<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="prospect_lead_freight_auto_sequence" model="freight.sequence">
            <field name="name">Prospect Lead</field>
            <field name="ir_model_id" ref="crm_prospect_lead.model_crm_prospect_lead"/>
            <field name="ir_field_id" ref="crm_prospect_lead.field_crm_prospect_lead__name"/>
            <field name="ir_sequence_id" ref="crm_prospect_lead.crm_prospect_lead_seq"/>
            <field name="sequence_format">LE</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="prospect_opportunity_freight_auto_sequence" model="freight.sequence">
            <field name="name">Prospect Opportunity</field>
            <field name="ir_model_id" ref="crm_prospect_lead.model_crm_prospect_opportunity"/>
            <field name="ir_field_id" ref="crm_prospect_lead.field_crm_prospect_opportunity__name"/>
            <field name="ir_sequence_id" ref="crm_prospect_lead.crm_prospect_opportunity_seq"/>
            <field name="sequence_format">OP</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

    </data>

</odoo>
