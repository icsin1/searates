<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="view_freight_service_job_document_tree_inherited" model="ir.ui.view">
        <field name="name">freight.service.job.document</field>
        <field name="model">freight.service.job.document</field>
        <field name="inherit_id" ref="fm_service_job.view_freight_service_job_doc_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='document_file']" position="after">
                <field name="is_publish" widget="boolean_toggle" />
            </xpath>
        </field>
    </record>
</odoo>
