<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="l10n_tds_tax_web_report" model="web.report">
            <field name="name">TDS Report</field>
            <field name="model_id" ref="account.model_account_move_line"/>
            <field name="data_match_domain" />
            <field name="filter_multi_company">selector</field>
            <field name="filter_date_type">date_range</field>
            <field name="default_opening_date">this_month</field>
            <field name="date_field" ref="account.field_account_move_line__date"/>
            <field name="report_handler_model_id" ref="l10n_in_tds_tcs.model_tds_report_handler"/>
            <field name="is_account_report" eval="True"/>
        </record>

        <record id="ics_report_l10n_tds_report_line" model="web.report.line">
            <field name="web_report_id" ref="l10n_tds_tax_web_report"/>
            <field name="name">TDS</field>
            <field name="code">TDS</field>
            <field name="sequence">1</field>
            <field name="hierarchy_level">1</field>
            <field name="group_total" eval="False"/>
            <field name="strict_on_date_range" eval="False"/>
            <field name="group_by_fields">section,id</field>
            <field name="action_id" ref="account.action_move_journal_line"/>
            <field name="action_field_id" ref="account.field_account_move_line__move_id"/>
        </record>

        <record id="ics_report_l10n_tds_report_title" model="web.report.expression">
            <field name="web_report_line_id" ref="ics_report_l10n_tds_report_line"/>
            <field name="name">title</field>
            <field name="computation_engine">python_code</field>
            <field name="formula_expression">_report_handler_tds_values</field>
            <field name="subformula_expression">section</field>
            <field name="value_type">string</field>
        </record>

        <record id="ics_report_l10n_tds_report_reference_number" model="web.report.expression">
            <field name="web_report_line_id" ref="ics_report_l10n_tds_report_line"/>
            <field name="name">reference_number</field>
            <field name="computation_engine">python_code</field>
            <field name="formula_expression">_report_handler_tds_values</field>
            <field name="subformula_expression">reference_number</field>
            <field name="value_type">string</field>
        </record>

        <record id="l10n_tds_tax_reference_number" model="web.report.column">
            <field name="web_report_id" ref="l10n_tds_tax_web_report"/>
            <field name="name">Reference Number</field>
            <field name="expression_label">reference_number</field>
            <field name="sequence">1</field>
            <field name="value_type">string</field>
        </record>

        <record id="ics_report_l10n_tds_report_pan_number" model="web.report.expression">
            <field name="web_report_line_id" ref="ics_report_l10n_tds_report_line"/>
            <field name="name">pan_number</field>
            <field name="computation_engine">python_code</field>
            <field name="formula_expression">_report_handler_tds_values</field>
            <field name="subformula_expression">pan_number</field>
            <field name="value_type">string</field>
        </record>

        <record id="l10n_tds_tax_pan_number" model="web.report.column">
            <field name="web_report_id" ref="l10n_tds_tax_web_report"/>
            <field name="name">PAN</field>
            <field name="expression_label">pan_number</field>
            <field name="sequence">2</field>
            <field name="value_type">string</field>
        </record>

        <record id="ics_report_l10n_tds_report_deductee_name" model="web.report.expression">
            <field name="web_report_line_id" ref="ics_report_l10n_tds_report_line"/>
            <field name="name">deductee_name</field>
            <field name="computation_engine">python_code</field>
            <field name="formula_expression">_report_handler_tds_values</field>
            <field name="subformula_expression">deductee_name</field>
            <field name="value_type">string</field>
        </record>

        <record id="l10n_tds_tax_deductee_name" model="web.report.column">
            <field name="web_report_id" ref="l10n_tds_tax_web_report"/>
            <field name="name">Name</field>
            <field name="expression_label">deductee_name</field>
            <field name="sequence">3</field>
            <field name="value_type">string</field>
        </record>

        <record id="ics_report_l10n_tds_report_bill_date" model="web.report.expression">
            <field name="web_report_line_id" ref="ics_report_l10n_tds_report_line"/>
            <field name="name">bill_date</field>
            <field name="computation_engine">python_code</field>
            <field name="formula_expression">_report_handler_tds_values</field>
            <field name="subformula_expression">bill_date</field>
            <field name="value_type">string</field>
        </record>

        <record id="l10n_tds_tax_bill_date" model="web.report.column">
            <field name="web_report_id" ref="l10n_tds_tax_web_report"/>
            <field name="name">Date</field>
            <field name="expression_label">bill_date</field>
            <field name="sequence">4</field>
            <field name="value_type">string</field>
        </record>

        <record id="ics_report_l10n_tds_report_total_amount" model="web.report.expression">
            <field name="web_report_line_id" ref="ics_report_l10n_tds_report_line"/>
            <field name="name">total_amount</field>
            <field name="computation_engine">python_code</field>
            <field name="formula_expression">_report_handler_tds_values</field>
            <field name="subformula_expression">total_amount</field>
            <field name="value_type">monetary</field>
        </record>

        <record id="l10n_tds_tax_total_amount" model="web.report.column">
            <field name="web_report_id" ref="l10n_tds_tax_web_report"/>
            <field name="name">Amount</field>
            <field name="expression_label">total_amount</field>
            <field name="sequence">5</field>
            <field name="value_type">monetary</field>
        </record>

        <record id="ics_report_l10n_tds_report_tds_amount" model="web.report.expression">
            <field name="web_report_line_id" ref="ics_report_l10n_tds_report_line"/>
            <field name="name">tds_amount</field>
            <field name="computation_engine">python_code</field>
            <field name="formula_expression">_report_handler_tds_values</field>
            <field name="subformula_expression">tds_amount</field>
            <field name="value_type">monetary</field>
        </record>

        <record id="l10n_tds_tax_tds_amount" model="web.report.column">
            <field name="web_report_id" ref="l10n_tds_tax_web_report"/>
            <field name="name">TDS</field>
            <field name="expression_label">tds_amount</field>
            <field name="sequence">6</field>
            <field name="value_type">monetary</field>
        </record>

        <record id="ics_report_l10n_tds_report_tax_rate" model="web.report.expression">
            <field name="web_report_line_id" ref="ics_report_l10n_tds_report_line"/>
            <field name="name">tax_rate</field>
            <field name="computation_engine">python_code</field>
            <field name="formula_expression">_report_handler_tds_values</field>
            <field name="subformula_expression">tax_rate</field>
            <field name="value_type">float</field>
        </record>

        <record id="l10n_tds_tax_rate" model="web.report.column">
            <field name="web_report_id" ref="l10n_tds_tax_web_report"/>
            <field name="name">Rate</field>
            <field name="expression_label">tax_rate</field>
            <field name="sequence">7</field>
            <field name="value_type">float</field>
            <field name="hide_if_zero" eval="True"/>
        </record>

    </data>

</odoo>
