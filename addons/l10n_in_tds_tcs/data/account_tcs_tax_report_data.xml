<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="india_tcs_report" model="account.tax.report">
        <field name="name">TCS Report</field>
        <field name="country_id" ref="base.in"/>
    </record>

    <record id="tcs_report_line_tax_line_206C_alcoholic" model="account.tax.report.line">
        <field name="name">Section 206C(1): Alcoholic Liquor for human consumption</field>
        <field name="code">TCS_SECTION_206C_ALCOHOLIC</field>
        <field name="sequence" eval="1"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1) Alcoholic Liquor</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_tendu" model="account.tax.report.line">
        <field name="name">Section 206C(1): Tendu leaves</field>
        <field name="code">TCS_SECTION_206C_TENDU</field>
        <field name="sequence" eval="2"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1) Tendu leaves</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_timber" model="account.tax.report.line">
        <field name="name">Section 206C(1): Timber obtained under a forest lease</field>
        <field name="code">TCS_SECTION_206C_TIMBER</field>
        <field name="sequence" eval="3"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1) Timber (forest lease)</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_timber_other" model="account.tax.report.line">
        <field name="name">Section 206C(1): Timber obtained by any mode other than under a forest lease</field>
        <field name="code">TCS_SECTION_206C_TIMBER_OTHER</field>
        <field name="sequence" eval="4"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1) Timber (other than under a forest lease)</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_other_forest" model="account.tax.report.line">
        <field name="name">Section 206C(1): Any other forest produce not being timber or tendu leaves</field>
        <field name="code">TCS_SECTION_206C_OTHER_FOREST</field>
        <field name="sequence" eval="5"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1) other forest produce</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_scrap" model="account.tax.report.line">
        <field name="name">Section 206C(1): Scrap</field>
        <field name="code">TCS_SECTION_206C_SCRAP</field>
        <field name="sequence" eval="6"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1) Scrap</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_minrals" model="account.tax.report.line">
        <field name="name">Section 206C(1): Minrals, being coal or lignite or iron ore</field>
        <field name="code">TCS_SECTION_206C_MINRALS</field>
        <field name="sequence" eval="7"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1) Minrals</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_parking_lot" model="account.tax.report.line">
        <field name="name">Section 206C(1C): Parking lot</field>
        <field name="code">TCS_SECTION_206C_PARKING</field>
        <field name="sequence" eval="8"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1C) Parking lot</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_toll_plaza" model="account.tax.report.line">
        <field name="name">Section 206C(1C): Toll plaza</field>
        <field name="code">TCS_SECTION_206C_TOLL_PLAZA</field>
        <field name="sequence" eval="9"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1C) Toll plaza</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_mining" model="account.tax.report.line">
        <field name="name">Section 206C(1C): Mining and quarrying</field>
        <field name="code">TCS_SECTION_206C_MINING</field>
        <field name="sequence" eval="10"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1C) Mining and quarrying</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_motor" model="account.tax.report.line">
        <field name="name">Section 206C(1F): Motor Vehicle</field>
        <field name="code">TCS_SECTION_206C_MOTOR</field>
        <field name="sequence" eval="11"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1F)</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_remittance" model="account.tax.report.line">
        <field name="name">Section 206C(1G): Sum of money (above 7 lakhs) for remittance out of India</field>
        <field name="code">TCS_SECTION_206C_REMITTANCE</field>
        <field name="sequence" eval="12"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1G) remittance out of India</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_overseas" model="account.tax.report.line">
        <field name="name">Section 206C(1G): Seller of an overseas tour program package</field>
        <field name="code">TCS_SECTION_206C_OVERSEAS</field>
        <field name="sequence" eval="13"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1G) overseas tour program</field>
    </record>

    <record id="tcs_report_line_tax_line_206C_sale_of_goods" model="account.tax.report.line">
        <field name="name">Section 206C(1H): Sale of Goods</field>
        <field name="code">TCS_SECTION_206C_SALE_OF_GOODS</field>
        <field name="sequence" eval="14"/>
        <field name="report_id" ref="india_tcs_report"/>
        <field name="tag_name">206C(1H)</field>
    </record>

</odoo>
