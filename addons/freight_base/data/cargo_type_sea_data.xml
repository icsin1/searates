<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sea -->
        <record id="cargo_type_sea_bbk" model="cargo.type">
            <field name="code">BBK</field>
            <field name="name">Break Bulk</field>
            <field name="transport_mode_id" ref="freight_base.transport_mode_sea"/>
            <field name="is_package_group" eval="True"/>
            <field name="sequence">1</field>
        </record>
        <record id="cargo_type_sea_blk" model="cargo.type">
            <field name="code">BLK</field>
            <field name="name">Bulk</field>
            <field name="transport_mode_id" ref="freight_base.transport_mode_sea"/>
            <field name="is_package_group" eval="True"/>
            <field name="sequence">2</field>
        </record>
        <record id="cargo_type_sea_fcl" model="cargo.type">
            <field name="code">FCL</field>
            <field name="name">Full Container Load</field>
            <field name="transport_mode_id" ref="freight_base.transport_mode_sea"/>
            <field name="sequence">3</field>
        </record>
        <record id="cargo_type_sea_lcl" model="cargo.type">
            <field name="code">LCL</field>
            <field name="name">Less Container Load</field>
            <field name="transport_mode_id" ref="freight_base.transport_mode_sea"/>
            <field name="is_package_group" eval="True"/>
            <field name="calculated_dimension_lwh" eval="True"/>
            <field name="sequence">4</field>
        </record>
        <record id="cargo_type_sea_lqd" model="cargo.type">
            <field name="code">LQD</field>
            <field name="name">Liquid</field>
            <field name="transport_mode_id" ref="freight_base.transport_mode_sea"/>
            <field name="is_package_group" eval="True"/>
            <field name="sequence">5</field>
        </record>
        <record id="cargo_type_sea_ror" model="cargo.type">
            <field name="code">ROR</field>
            <field name="name">Roll On/Roll Off</field>
            <field name="transport_mode_id" ref="freight_base.transport_mode_sea"/>
            <field name="is_package_group" eval="True"/>
            <field name="sequence">6</field>
        </record>
        <record id="cargo_type_sea_courier" model="cargo.type">
            <field name="code">CR</field>
            <field name="name">Courier</field>
            <field name="transport_mode_id" ref="freight_base.transport_mode_sea"/>
            <field name="is_package_group" eval="True"/>
            <field name="sequence">6</field>
            <field name="is_courier_shipment" eval="True"/>
        </record>
    </data>
    <function name="write" model="ir.model.data">
    <function name="search" model="ir.model.data">
        <value eval="[('module', '=', 'freight_base'), ('name', '=', 'cargo_type_sea_lcl')]"/>
    </function>
        <value eval="{'noupdate': False}" />
    </function>

    <record id="freight_base.cargo_type_sea_lcl" model="cargo.type">
        <field name="calculated_dimension_lwh" eval="True"/>
    </record>
    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'freight_base'), ('name', '=', 'cargo_type_sea_lcl')]"/>
        </function>
        <value eval="{'noupdate': True}" />
    </function>
</odoo>
