<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'base'), ('model', '=', 'res.country')]"/>
        </function>
        <value eval="{'noupdate': False}"/>
    </function>

    <function model="res.country" name="write">
        <value model="res.country" eval="obj().search([('enforce_cities', '=', False)]).ids" />
        <value eval="{'enforce_cities': True}"/>
    </function>

    <record id="base.in" model="res.country">
        <field name="vat_report_label">GST</field>
    </record>

    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'base'), ('model', '=', 'res.country')]"/>
        </function>
        <value eval="{'noupdate': True}"/>
    </function>

    <record id="an" model="res.country">
        <field name="name">Netherlands Antilles</field>
        <field name="code">AN</field>
        <field name="currency_id" ref="base.ANG"/>
        <field eval="599" name="phone_code"/>
    </record>

</odoo>
