<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="shipment_type_export" model="shipment.type">
            <field name="code">EXP</field>
            <field name="name">Export</field>
            <field name="sequence">1</field>
        </record>
        <record id="shipment_type_import" model="shipment.type">
            <field name="code">IMP</field>
            <field name="name">Import</field>
            <field name="sequence">2</field>
        </record>
        <record id="shipment_type_cross_trade" model="shipment.type">
            <field name="code">CTD</field>
            <field name="name">Cross Trade</field>
            <field name="sequence">3</field>
        </record>
        <record id="shipment_type_domestic" model="shipment.type">
            <field name="code">DOM</field>
            <field name="name">Domestic</field>
            <field name="sequence">3</field>
            <field name="is_courier_shipment" eval="True"/>
        </record>
         <record id="shipment_type_reexport" model="shipment.type">
            <field name="code">REXP</field>
            <field name="name">Re-Export</field>
            <field name="sequence">4</field>
            <field name="active" eval="False"/>
        </record>
    </data>
</odoo>
