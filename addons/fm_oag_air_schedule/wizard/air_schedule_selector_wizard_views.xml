<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_schedule_selector_oag_wizard_form" model="ir.ui.view">
        <field name="name">air.schedule.selector.oag.wizard.form</field>
        <field name="model">air.schedule.selector.wizard</field>
        <field name="inherit_id" ref="fm_air_schedule.view_air_schedule_selector_wizard_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@type='special']" position="before">
                <button name="action_import_schedule" string="Import Schedules" type="object" class="btn btn-info" title="Import Global Schedules"/>
            </xpath>
        </field>
    </record>
</odoo>