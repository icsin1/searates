<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="om_vat_return_report" model="account.tax.report">
        <field name="name">Vat Return Report</field>
        <field name="country_id" ref="base.om"/>
    </record>

    <record id="om_vat_return_report_base_supplies_sultanate_1" model="account.tax.report.line">
        <field name="name">1. Supplies in the Sultanate of Oman (Base)</field>
        <field name="code">SUPPLIES_SULTANATE_BASE_1</field>
        <field name="formula">SUPPLIES_SULTANATE_BASE_1A + SUPPLIES_SULTANATE_BASE_1B + SUPPLIES_SULTANATE_BASE_1C + SUPPLIES_SULTANATE_BASE_1D + SUPPLIES_SULTANATE_BASE_1E + SUPPLIES_SULTANATE_BASE_1F</field>
        <field name="sequence" eval="1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_supplies_sultanate_1a" model="account.tax.report.line">
        <field name="name">1(a) Supplies of goods / services taxed at 5% (Base)</field>
        <field name="code">SUPPLIES_SULTANATE_BASE_1A</field>
        <field name="tag_name">BASE VAT 5%</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_base_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_supplies_sultanate_1b" model="account.tax.report.line">
        <field name="name">1(b) Supplies of goods / services taxed at 0% (Base)</field>
        <field name="code">SUPPLIES_SULTANATE_BASE_1B</field>
        <field name="tag_name">BASE VAT 0%</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_base_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_supplies_sultanate_1c" model="account.tax.report.line">
        <field name="name">1(c) Supplies of goods / services tax exempt (Base)</field>
        <field name="code">SUPPLIES_SULTANATE_BASE_1C</field>
        <field name="tag_name">BASE VAT Exempt</field>
        <field name="sequence" eval="3"/>
        <field name="parent_id" ref="om_vat_return_report_base_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_supplies_sultanate_1d" model="account.tax.report.line">
        <field name="name">1(d) Supplies of goods, tax levy shifted to recipient inside GCC (supplies made by you that are subject to Reverse Charge Mechanism) (Base)</field>
        <field name="code">SUPPLIES_SULTANATE_BASE_1D</field>
        <field name="sequence" eval="4"/>
        <field name="parent_id" ref="om_vat_return_report_base_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_supplies_sultanate_1e" model="account.tax.report.line">
        <field name="name">1(e) Supplies of services, tax levy shifted to recipient inside GCC (supplies made by you that are subject to Reverse Charge Mechanism) (Base)</field>
        <field name="code">SUPPLIES_SULTANATE_BASE_1E</field>
        <field name="sequence" eval="5"/>
        <field name="parent_id" ref="om_vat_return_report_base_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_supplies_sultanate_1f" model="account.tax.report.line">
        <field name="name">1(f) Supply of goods as per profit margin scheme (Base)</field>
        <field name="code">SUPPLIES_SULTANATE_BASE_1F</field>
        <field name="sequence" eval="6"/>
        <field name="parent_id" ref="om_vat_return_report_base_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_purchase_reverse_charge_2" model="account.tax.report.line">
        <field name="name">2. Purchases subject to Reverse Charge Mechanism (Base)</field>
        <field name="code">PURCHASE_REVERSE_CHARGE_BASE_2</field>
        <field name="formula">PURCHASE_REVERSE_CHARGE_BASE_2A + PURCHASE_REVERSE_CHARGE_BASE_2B</field>
        <field name="sequence" eval="2"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_purchase_reverse_charge_2a" model="account.tax.report.line">
        <field name="name">2(a) Purchases from the GCC subject to Reverse Charge Mechanism (Base)</field>
        <field name="code">PURCHASE_REVERSE_CHARGE_BASE_2A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_base_purchase_reverse_charge_2"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_purchase_reverse_charge_2b" model="account.tax.report.line">
        <field name="name">2(b) Purchases from outside of GCC subject to Reverse Charge Mechanism (Base)</field>
        <field name="code">PURCHASE_REVERSE_CHARGE_BASE_2B</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_base_purchase_reverse_charge_2"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_supplies_outside_3" model="account.tax.report.line">
        <field name="name">3. Supplies to countries outside of Oman (Base)</field>
        <field name="code">SUPPLIES_OUTSIDE_BASE_3</field>
        <field name="formula">EXPORT_BASE_3A</field>
        <field name="sequence" eval="3"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_export_3a" model="account.tax.report.line">
        <field name="name">3(a) Exports (Base)</field>
        <field name="code">EXPORT_BASE_3A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_base_supplies_outside_3"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_import_goods_4" model="account.tax.report.line">
        <field name="name">4. Import of Goods (Base)</field>
        <field name="code">IMPORT_GOODS_BASE_4</field>
        <field name="formula">IMPORT_GOODS_BASE_4A + IMPORT_GOODS_BASE_4B</field>
        <field name="sequence" eval="4"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_import_goods_4a" model="account.tax.report.line">
        <field name="name">4(a) Import of Goods (Postponed payment) (Base)</field>
        <field name="code">IMPORT_GOODS_BASE_4A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_base_import_goods_4"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_import_goods_4b" model="account.tax.report.line">
        <field name="name">4(b) Total goods imported (Base)</field>
        <field name="code">IMPORT_GOODS_BASE_4B</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_base_import_goods_4"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_total_vat_due_5" model="account.tax.report.line">
        <field name="name">5. Total VAT due (Base)</field>
        <field name="code">TOTAL_VAT_DUE_BASE_5</field>
        <field name="formula">TOTAL_VAT_DUE_BASE_5A + TOTAL_VAT_DUE_BASE_5B</field>
        <field name="sequence" eval="5"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_total_vat_due_5a" model="account.tax.report.line">
        <field name="name">5(a) Total VAT due under (1(a)+1(f)+2(a)+2(b)+4(a)) (Base)</field>
        <field name="code">TOTAL_VAT_DUE_BASE_5A</field>
        <field name="formula">SUPPLIES_SULTANATE_BASE_1A + SUPPLIES_SULTANATE_BASE_1F + PURCHASE_REVERSE_CHARGE_BASE_2A +PURCHASE_REVERSE_CHARGE_BASE_2B + IMPORT_GOODS_BASE_4A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_base_total_vat_due_5"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_total_vat_due_5b" model="account.tax.report.line">
        <field name="name">5(b) Adjustment of VAT due (Base)</field>
        <field name="code">TOTAL_VAT_DUE_BASE_5B</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_base_total_vat_due_5"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_input_vat_credit_6" model="account.tax.report.line">
        <field name="name">6. Input VAT credit (Base)</field>
        <field name="code">INPUT_VAT_CREDIT_BASE_6</field>
        <field name="formula">INPUT_VAT_CREDIT_BASE_6A + INPUT_VAT_CREDIT_BASE_6B + INPUT_VAT_CREDIT_BASE_6C + INPUT_VAT_CREDIT_BASE_6D</field>
        <field name="sequence" eval="6"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_input_vat_credit_6a" model="account.tax.report.line">
        <field name="name">6(a) Purchases (except import of goods) (Base)</field>
        <field name="code">INPUT_VAT_CREDIT_BASE_6A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_base_input_vat_credit_6"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_input_vat_credit_6b" model="account.tax.report.line">
        <field name="name">6(b) Import of goods (2(b)+4(a)+4(b)) (Base)</field>
        <field name="code">INPUT_VAT_CREDIT_BASE_6B</field>
        <field name="formula">PURCHASE_REVERSE_CHARGE_BASE_2B + IMPORT_GOODS_BASE_4A + IMPORT_GOODS_BASE_4B</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_base_input_vat_credit_6"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_input_vat_credit_6c" model="account.tax.report.line">
        <field name="name">6(c) VAT on acquisition of fixed assets (Base)</field>
        <field name="code">INPUT_VAT_CREDIT_BASE_6C</field>
        <field name="sequence" eval="3"/>
        <field name="parent_id" ref="om_vat_return_report_base_input_vat_credit_6"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_input_vat_credit_6d" model="account.tax.report.line">
        <field name="name">6(d) Adjustment of input VAT credit (Base)</field>
        <field name="code">INPUT_VAT_CREDIT_BASE_6D</field>
        <field name="sequence" eval="4"/>
        <field name="parent_id" ref="om_vat_return_report_base_input_vat_credit_6"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_tax_liability_7" model="account.tax.report.line">
        <field name="name">7. Tax liability calculation (Base)</field>
        <field name="code">TAX_LIABILITY_BASE_7</field>
        <field name="formula">TAX_LIABILITY_BASE_7A + TAX_LIABILITY_BASE_7B + TAX_LIABILITY_BASE_7C</field>
        <field name="sequence" eval="7"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_tax_liability_7a" model="account.tax.report.line">
        <field name="name">7(a) Total VAT due (5(a) + 5(b)) (Base)</field>
        <field name="code">TAX_LIABILITY_BASE_7A</field>
        <field name="formula">TOTAL_VAT_DUE_BASE_5A + TOTAL_VAT_DUE_BASE_5B</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_base_tax_liability_7"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_tax_liability_7b" model="account.tax.report.line">
        <field name="name">7(b) Total input VAT Credit (6(a)+6(b)+6(c)+6(d)) (Base)</field>
        <field name="code">TAX_LIABILITY_BASE_7B</field>
        <field name="formula">INPUT_VAT_CREDIT_BASE_6A + INPUT_VAT_CREDIT_BASE_6B + INPUT_VAT_CREDIT_BASE_6C + INPUT_VAT_CREDIT_BASE_6D</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_base_tax_liability_7"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_base_tax_liability_7c" model="account.tax.report.line">
        <field name="name">7(c) Total (7(a) - 7(b)) (Base)</field>
        <field name="code">TAX_LIABILITY_BASE_7C</field>
        <field name="formula">TAX_LIABILITY_BASE_7A - TAX_LIABILITY_BASE_7B</field>
        <field name="sequence" eval="3"/>
        <field name="parent_id" ref="om_vat_return_report_base_tax_liability_7"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_supplies_sultanate_1" model="account.tax.report.line">
        <field name="name">1. Supplies in the Sultanate of Oman (Vat)</field>
        <field name="code">SUPPLIES_SULTANATE_VAT_1</field>
        <field name="formula">SUPPLIES_SULTANATE_VAT_1A + SUPPLIES_SULTANATE_VAT_1B + SUPPLIES_SULTANATE_VAT_1C + SUPPLIES_SULTANATE_VAT_1D + SUPPLIES_SULTANATE_VAT_1E + SUPPLIES_SULTANATE_VAT_1F</field>
        <field name="sequence" eval="8"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_supplies_sultanate_1a" model="account.tax.report.line">
        <field name="name">1(a) Supplies of goods / services taxed at 5% (Vat)</field>
        <field name="code">SUPPLIES_SULTANATE_VAT_1A</field>
        <field name="tag_name">VAT 5%</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_vat_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_supplies_sultanate_1b" model="account.tax.report.line">
        <field name="name">1(b) Supplies of goods / services taxed at 0% (Vat)</field>
        <field name="code">SUPPLIES_SULTANATE_VAT_1B</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_vat_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_supplies_sultanate_1c" model="account.tax.report.line">
        <field name="name">1(c) Supplies of goods / services tax exempt (Vat)</field>
        <field name="code">SUPPLIES_SULTANATE_VAT_1C</field>
        <field name="sequence" eval="3"/>
        <field name="parent_id" ref="om_vat_return_report_vat_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_supplies_sultanate_1d" model="account.tax.report.line">
        <field name="name">1(d) Supplies of goods, tax levy shifted to recipient inside GCC (supplies made by you that are subject to Reverse Charge Mechanism) (Vat)</field>
        <field name="code">SUPPLIES_SULTANATE_VAT_1D</field>
        <field name="sequence" eval="4"/>
        <field name="parent_id" ref="om_vat_return_report_vat_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_supplies_sultanate_1e" model="account.tax.report.line">
        <field name="name">1(e) Supplies of services, tax levy shifted to recipient inside GCC (supplies made by you that are subject to Reverse Charge Mechanism) (Vat)</field>
        <field name="code">SUPPLIES_SULTANATE_VAT_1E</field>
        <field name="sequence" eval="5"/>
        <field name="parent_id" ref="om_vat_return_report_vat_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_supplies_sultanate_1f" model="account.tax.report.line">
        <field name="name">1(f) Supply of goods as per profit margin scheme (Vat)</field>
        <field name="code">SUPPLIES_SULTANATE_VAT_1F</field>
        <field name="sequence" eval="6"/>
        <field name="parent_id" ref="om_vat_return_report_vat_supplies_sultanate_1"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_purchase_reverse_charge_2" model="account.tax.report.line">
        <field name="name">2. Purchases subject to Reverse Charge Mechanism (Vat)</field>
        <field name="code">PURCHASE_REVERSE_CHARGE_VAT_2</field>
        <field name="formula">PURCHASE_REVERSE_CHARGE_VAT_2A + PURCHASE_REVERSE_CHARGE_VAT_2B</field>
        <field name="sequence" eval="9"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_purchase_reverse_charge_2a" model="account.tax.report.line">
        <field name="name">2(a) Purchases from the GCC subject to Reverse Charge Mechanism (Vat)</field>
        <field name="code">PURCHASE_REVERSE_CHARGE_VAT_2A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_vat_purchase_reverse_charge_2"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_purchase_reverse_charge_2b" model="account.tax.report.line">
        <field name="name">2(b) Purchases from outside of GCC subject to Reverse Charge Mechanism (Vat)</field>
        <field name="code">PURCHASE_REVERSE_CHARGE_VAT_2B</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_vat_purchase_reverse_charge_2"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_supplies_outside_3" model="account.tax.report.line">
        <field name="name">3. Supplies to countries outside of Oman (Vat)</field>
        <field name="code">SUPPLIES_OUTSIDE_VAT_3</field>
        <field name="formula">EXPORT_BASE_3A</field>
        <field name="sequence" eval="10"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_export_3a" model="account.tax.report.line">
        <field name="name">3(a) Exports (Vat)</field>
        <field name="code">EXPORT_VAT_3A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_vat_supplies_outside_3"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_import_goods_4" model="account.tax.report.line">
        <field name="name">4. Import of Goods (Vat)</field>
        <field name="code">IMPORT_GOODS_VAT_4</field>
        <field name="formula">IMPORT_GOODS_VAT_4A + IMPORT_GOODS_VAT_4B</field>
        <field name="sequence" eval="11"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_import_goods_4a" model="account.tax.report.line">
        <field name="name">4(a) Import of Goods (Postponed payment) (Vat)</field>
        <field name="code">IMPORT_GOODS_VAT_4A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_vat_import_goods_4"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_import_goods_4b" model="account.tax.report.line">
        <field name="name">4(b) Total goods imported (Vat)</field>
        <field name="code">IMPORT_GOODS_VAT_4B</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_vat_import_goods_4"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_total_vat_due_5" model="account.tax.report.line">
        <field name="name">5. Total VAT due (Vat)</field>
        <field name="code">TOTAL_VAT_DUE_VAT_5</field>
        <field name="formula">TOTAL_VAT_DUE_VAT_5A + TOTAL_VAT_DUE_VAT_5B</field>
        <field name="sequence" eval="12"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_total_vat_due_5a" model="account.tax.report.line">
        <field name="name">5(a) Total VAT due under (1(a)+1(f)+2(a)+2(b)+4(a)) (Vat)</field>
        <field name="code">TOTAL_VAT_DUE_VAT_5A</field>
        <field name="formula">SUPPLIES_SULTANATE_VAT_1A + SUPPLIES_SULTANATE_VAT_1F + PURCHASE_REVERSE_CHARGE_VAT_2A + PURCHASE_REVERSE_CHARGE_VAT_2B + IMPORT_GOODS_VAT_4A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_vat_total_vat_due_5"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_total_vat_due_5b" model="account.tax.report.line">
        <field name="name">5(b) Adjustment of VAT due (Vat)</field>
        <field name="code">TOTAL_VAT_DUE_VAT_5B</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_vat_total_vat_due_5"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_input_vat_credit_6" model="account.tax.report.line">
        <field name="name">6. Input VAT credit (Vat)</field>
        <field name="code">INPUT_VAT_CREDIT_VAT_6</field>
        <field name="formula">INPUT_VAT_CREDIT_VAT_6A + INPUT_VAT_CREDIT_VAT_6B + INPUT_VAT_CREDIT_VAT_6C + INPUT_VAT_CREDIT_VAT_6D</field>
        <field name="sequence" eval="13"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_input_vat_credit_6a" model="account.tax.report.line">
        <field name="name">6(a) Purchases (except import of goods) (Vat)</field>
        <field name="code">INPUT_VAT_CREDIT_VAT_6A</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_vat_input_vat_credit_6"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_input_vat_credit_6b" model="account.tax.report.line">
        <field name="name">6(b) Import of goods (2(b)+4(a)+4(b)) (Vat)</field>
        <field name="code">INPUT_VAT_CREDIT_VAT_6B</field>
        <field name="formula">PURCHASE_REVERSE_CHARGE_VAT_2B + IMPORT_GOODS_VAT_4A + IMPORT_GOODS_VAT_4B</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_vat_input_vat_credit_6"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_input_vat_credit_6c" model="account.tax.report.line">
        <field name="name">6(c) VAT on acquisition of fixed assets</field>
        <field name="code">INPUT_VAT_CREDIT_VAT_6C</field>
        <field name="sequence" eval="3"/>
        <field name="parent_id" ref="om_vat_return_report_vat_input_vat_credit_6"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_input_vat_credit_6d" model="account.tax.report.line">
        <field name="name">6(d) Adjustment of input VAT credit</field>
        <field name="code">INPUT_VAT_CREDIT_VAT_6D</field>
        <field name="sequence" eval="4"/>
        <field name="parent_id" ref="om_vat_return_report_vat_input_vat_credit_6"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_tax_liability_7" model="account.tax.report.line">
        <field name="name">7. Tax liability calculation (Vat)</field>
        <field name="code">TAX_LIABILITY_VAT_7</field>
        <field name="formula">TAX_LIABILITY_VAT_7A + TAX_LIABILITY_VAT_7B + TAX_LIABILITY_VAT_7C</field>
        <field name="sequence" eval="14"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_tax_liability_7a" model="account.tax.report.line">
        <field name="name">7(a) Total VAT due (5(a) + 5(b)) (VAT)</field>
        <field name="code">TAX_LIABILITY_VAT_7A</field>
        <field name="formula">TOTAL_VAT_DUE_VAT_5A + TOTAL_VAT_DUE_VAT_5B</field>
        <field name="sequence" eval="1"/>
        <field name="parent_id" ref="om_vat_return_report_vat_tax_liability_7"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_tax_liability_7b" model="account.tax.report.line">
        <field name="name">7(b) Total input VAT Credit (6(a)+6(b)+6(c)+6(d)) (Vat)</field>
        <field name="code">TAX_LIABILITY_VAT_7B</field>
        <field name="formula">INPUT_VAT_CREDIT_VAT_6A + INPUT_VAT_CREDIT_VAT_6B + INPUT_VAT_CREDIT_VAT_6C + INPUT_VAT_CREDIT_VAT_6D</field>
        <field name="sequence" eval="2"/>
        <field name="parent_id" ref="om_vat_return_report_vat_tax_liability_7"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

    <record id="om_vat_return_report_vat_tax_liability_7c" model="account.tax.report.line">
        <field name="name">7(c) Total (7(a) - 7(b)) (Vat)</field>
        <field name="code">TAX_LIABILITY_VAT_7C</field>
        <field name="formula">TAX_LIABILITY_VAT_7A - TAX_LIABILITY_VAT_7B</field>
        <field name="sequence" eval="3"/>
        <field name="parent_id" ref="om_vat_return_report_vat_tax_liability_7"/>
        <field name="report_id" ref="om_vat_return_report"/>
    </record>

