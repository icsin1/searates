<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <record forcecreate="True" id="ir_cron_web_notification" model="ir.cron">
            <field name="name">Web Notification</field>
            <field name="model_id" ref="model_web_notification_manager"/>
            <field name="state">code</field>
            <field name="code">model._send_web_notification()</field>
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
        </record>
    </data>
</odoo>
