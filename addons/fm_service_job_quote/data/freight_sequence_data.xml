<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="service_job_freight_product" model="freight.product">
            <field name="name">Service Job Freight Product</field>
            <field name="model_id" ref="fm_quote.model_shipment_quote"/>
            <field name="match_domain">[['quote_for','=','job']]</field>
        </record>

        <record id="sequence_freight_service_job_quotation" model="ir.sequence">
            <field name="name">Freight Service-Job Quotation Sequence</field>
            <field name="code">freight.service.job.quotation.sequence</field>
            <field name="prefix">/%(year)s/</field>
            <field name="padding">3</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

    </data>
</odoo>
