<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_module_module_from_view_inherit" model="ir.ui.view">
        <field name="name">ir.module.module.form.inherit.invisible</field>
        <field name="model">ir.module.module</field>
        <field name="inherit_id" ref="base.module_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='technical_data']/group/field[@name='state']" position="after">
                <field name="active" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>